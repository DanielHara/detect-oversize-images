{
    "manifest_version": 3,
    "name": "Detect too-large images",
    "description": "Detect images which are too large",
    "version": "1.0",
    "action": {
      "default_icon": "favicon-16x16.png",
      "default_title": "Detect too large images"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}
