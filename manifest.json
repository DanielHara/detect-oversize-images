{
    "manifest_version": 3,
    "name": "Detect oversize images",
    "description": "Detect images which are too large",
    "version": "0.1",
    "action": {
      "default_icon": "favicon-16x16.png",
      "default_title": "Detect too large images"
    },
    "icons": {
        "16": "favicon-16x16.png",
        "48": "favicon-48x48.png",
        "128": "favicon-128x128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}
